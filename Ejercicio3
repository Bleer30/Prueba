using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Ejercicio3
{
    class Practica3
    {
        static void Main(string[] args)
        {
           Console.WriteLine("Escribe el primer numero:");
           int num1=Convert.ToInt32(Console.ReadLine()); 
           Console.Write("Escribe el segundo digito:");
           int num2= Convert.ToInt32(Console.ReadLine());
           if (num1>=num2){
               Console.WriteLine("El primer numero que ingreso es el mayor");
            }else{
               Console.WriteLine("El segundo numero que ingreso es el mayor");
           }
                
            Console.ReadLine();//Necesario
        }//main
    }//Class final
}//El final
